    "example_worker": "ts-node src/example_worker.ts"
    "test": "jest --silent",
    "coverage": "jest --coverage --silent"
  },
  "keywords": [],
  "author": "",
  "license": "UNLICENSED",
  "description": "",
  "devDependencies": {
    "@types/amqplib": "^0.10.5",
    "@types/node": "^22.7.2",
    "jest": "^29.7.0",
    "jest-mock-extended": "^4.0.0-beta1",
    "mongodb": "^6.9.0",
    "nodemon": "^3.1.7",
    "ts-node": "^10.9.2",
    "tsc-watch": "^6.2.0",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "@types/jest": "^29.5.14",
    "@types/spotify-web-api-node": "^5.0.11",
    "acorn": "^8.12.1",
    "acorn-walk": "^8.3.4",
    "amqplib": "^0.10.4",
    "arg": "^4.1.3",
    "axios": "^1.7.7",
    "create-require": "^1.1.1",
    "diff": "^4.0.2",
    "discord.js": "^14.16.3",
    "dotenv": "^16.4.5",
    "googleapis": "^144.0.0",
    "liquidjs": "^10.18.0",
    "make-error": "^1.3.6",
    "spotify-web-api-node": "^5.0.2",
    "ts-jest": "^29.2.5",
    "tsconfig-paths": "^4.2.0",
    "undici-types": "^6.19.8",
    "v8-compile-cache-lib": "^3.0.1",
    "yn": "^3.1.1"
  },
  "ts-node": {
    "require": [
      "tsconfig-paths/register"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/src/$1"
    },
    "rootDir": ".",
    "testRegex": "(/__tests__/.*|(\\.|/))(test|spec)\\.ts$",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/entrypoint.ts",
      "!src/example_worker.ts",
      "!src/actions/spotify/on-spotify-playlist-update.ts"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/",
      "/src/entrypoint.ts"
    ]
  }
}
